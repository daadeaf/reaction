@import 'partials/_home'

body
  padding-top 56px

.container
  max-width 960px

footer
  .app-logo
    max-height 5rem


// extra utilities

.border-top
  border-top 1px solid #e5e5e5

.border-bottom
  border-bottom 1px solid #e5e5e5

.flex-equal > *
  -ms-flex 1
  -webkit-box-flex 1
  flex 1

@media (min-width 768px)
  .flex-md-equal > *
    -ms-flex 1
    -webkit-box-flex 1
    flex 1

.overflow-hidden
  overflow hidden
